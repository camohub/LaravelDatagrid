{

}


class DatagridConstructException extends \Exception
{
	protected $message = 'Argument 1 passed to Camohub\LaravelDatagrid\Datagrid::__construct() must be an instance of Illuminate\Database\Eloquent\Builder or Illuminate\Database\Query\Builder';
}